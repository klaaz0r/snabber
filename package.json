{
  "name": "snabber",
  "version": "0.0.1",
  "description": "simple snabbdom helpers",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src/index.js -o dist/index.js",
    "commit": "git-cz",
    "test": "mocha src/index.test.js -w --compilers js:babel/register",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "klaaz0r",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "snabbdom": "^0.6.3"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "chai": "3.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "1.0.4",
    "cz-conventional-changelog": "1.1.0",
    "ghooks": "0.3.2",
    "istanbul": "0.3.18",
    "mocha": "2.2.5",
    "semantic-release": "^4.2.1",
    "snabbdom-to-html": "^3.0.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  }
}
